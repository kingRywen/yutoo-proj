{
  "name": "erp-product-web",
  "description": "A Vue.js project",
  "author": "yutoo",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --progress --hot --config build/webpack.dev.conf.js",
    "dev:sin": "cross-env NODE_ENV=development SINGLE_ENV=1 webpack-dev-server --inline --progress --hot --config build/webpack.dev.conf.js",
    "mock": "node ./mock/mock.js",
    "start": "concurrently \"npm run dev\" \"npm run mock\"",
    "debug": "webpack-dev-server --inline --hot --config build/webpack.dev.conf.js",
    "build:test": "cross-env NODE_ENV=test_env webpack --progress --config build/webpack.prod.conf.js",
    "build:con": "cross-env NODE_ENV=test webpack --progress --config build/webpack.prod.conf.js",
    "build:prod": "cross-env NODE_ENV=production webpack --progress --config build/webpack.prod.conf.js",
    "analyz": "cross-env NODE_ENV=production npm_config_report=true webpack --progress --config build/webpack.prod.conf.js"
  },
  "browserslist": [
    "defaults",
    "not ie < 11",
    "last 2 versions",
    "> 1%",
    "iOS 7",
    "last 3 iOS versions"
  ],
