{
  "name": "@yutoo/erp2-components",
  "version": "0.0.1",
  "description": "erp2通用组件",
  "main": "dist/erp2-components.umd.min.js",
  "keyword": "yutoo",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve --model development",
    "lib": "vue-cli-service serve --model lib",
    "build": "vue-cli-service build --model production",
    "build:package": "vue-cli-service build --target lib --name erp2-components --dest dist ./src/components/index.js",
    "build-watch": "vue-cli-service --watch build --target lib --name erp2-components --dest dist ./src/components/index.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "vue": "^2.5.17",
    "vue-clipboard2": "^0.3.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.17"
  },
  "resolutions": {
    "webpack-dev-server": "3.4.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
