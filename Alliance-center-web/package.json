{
  "name": "league-center",
  "version": "0.1.0",
  "description": "联盟中心端",
  "private": false,
  "scripts": {
    "build": "node --inspect-brk=10032 ./node_modules/@vue/cli-service/bin/vue-cli-service.js build --modern",
    "serve": "vue-cli-service serve",
    "build:test": "vue-cli-service build --modern --mode test",
    "build:online": "vue-cli-service build --modern --mode online",
    "build:all": "concurrently \"yarn build:test\" \"yarn build:online\"",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@vue-materials/basic-container": "^1.0.0",
    "@yutoo/yutoo": "^0.1.8",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "clipboard": "^2.0.4",
    "concurrently": "^4.1.0",
    "echarts": "^4.4.0",
    "element-ui": "^2.6.0",
    "global": "^4.3.2",
    "lodash": "^4.17.11",
    "normalize.css": "^8.0.0",
    "throttle-debounce": "^2.1.0",
    "vue": "^2.5.17",
    "vue-clipboard2": "^0.3.1",
    "vue-echarts": "^4.0.4",
    "vue-lazyload": "^1.2.6",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-plugin-eslint": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "element-theme-chalk": "2.4.9",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "style-resources-loader": "^1.2.1",
    "svg-sprite-loader": "^4.1.3",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "vue/require-v-for-key": "off",
      "no-unused-vars": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8",
    "ie 10"
  ]
}
